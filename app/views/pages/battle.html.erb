<img src="data:image/jpeg;base64," alt="">

<div class="health-bar">
  <% @counter_1 = 1 %>
  <% @pokemon_1.hp.times do %>
    <div class="health-gauge health-<%= @counter_1 %>"></div>
    <% @counter_1 += 1 %>
  <% end %>
</div>

<div id="hp" class="invisible"><%= @pokemon_1.hp %></div>

<div>
  <h4><%= @pokemon_1.name %></h4>
</div>

<div class="health-bar">
  <% @counter_2 = 1 %>
  <% @pokemon_2.hp.times do %>
    <div class="health-gauge health-<%= @counter_2 %>"></div>
    <% @counter_2 += 1 %>
  <% end %>
</div>

<div id="hp" class="invisible"><%= @pokemon_2.hp %></div>

<div>
  <h4><%= @pokemon_2.name %></h4>
</div>

<div id="execute"></div>


<div class="battle-container">
  <div class="top-half">
    <div class="top-left">

    </div>
    <div class="top-right">
      <img src="<%= @pokemon_2.sprite_front %>" alt="Pokemon Image">
    </div>
    <div class="bottom-left">
      <img src="<%= @pokemon_1.sprite_back %>" alt="Pokemon Image">
    </div>
    <div class="bottom-right">

    </div>
  </div>
  <div class="bottom-half">
    <div class="description"></div>
    <div class="moves">
      <div class="move active">
        <p><%= @move_1.name %></p>
        <div class="move-data">
          <div class="type <%= @move_1.move_type %>"><%= @move_1.move_type %></div>
          <div class="pp">PP <%= @move_1.pp %>/<%= @move_1.pp %></div>
        </div>
      </div>
      <div class="move">
        <p><%= @move_2.name %></p>
        <div class="move-data">
          <div class="type <%= @move_2.move_type %>"><%= @move_2.move_type %></div>
          <div class="pp">PP <%= @move_2.pp %>/<%= @move_2.pp %></div>
        </div>
      </div>
      <div class="move">
        <p><%= @move_3.name %></p>
        <div class="move-data">
          <div class="type <%= @move_3.move_type %>"><%= @move_3.move_type %></div>
          <div class="pp">PP <%= @move_3.pp %>/<%= @move_3.pp %></div>
        </div>
      </div>
      <div class="move">
        <p><%= @move_4.name %></p>
        <div class="move-data">
          <div class="type <%= @move_4.move_type %>"><%= @move_4.move_type %></div>
          <div class="pp">PP <%= @move_4.pp %>/<%= @move_4.pp %></div>
        </div>
      </div>
    </div>
  </div>
</div>

<%= javascript_pack_tag 'battle' %>
